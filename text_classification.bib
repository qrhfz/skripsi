	title = {Dijkstra's and {A}-{Star} in {Finding} the {Shortest} {Path}: a {Tutorial}},
	isbn = {978-1-72819-792-0},
	shorttitle = {Dijkstra's and {A}-{Star} in {Finding} the {Shortest} {Path}},
