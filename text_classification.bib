@inproceedings{yovine_checkdroid_2017,
	address = {Buenos Aires, Argentina},
	title = {{CheckDroid}: {A} {Tool} for {Automated} {Detection} of {Bad} {Practices} in {Android} {Applications} {Using} {Taint} {Analysis}},
	isbn = {978-1-5386-2669-6},
	shorttitle = {{CheckDroid}},
	url = {http://ieeexplore.ieee.org/document/7972733/},
	doi = {10.1109/MOBILESoft.2017.37},
	abstract = {Today, Android runs on more than 80\% of smartphones and tablets in the market. Although a guide to good programming practices exists, it is not at all enforced by current IDEs. This work presents CheckDroid, a tool towards automatically detecting bad patterns early in the development cycle. Its originality relies on the idea of mapping bad practices into appropriate paths in the code which can be found by static taint analysis. The main beneﬁt for developers consists in the ability to early, simply and quickly clean up the code of potential causes of runtime errors. More speciﬁc tools can be used later for deeper veriﬁcation. Preliminary experimental evaluation shows that CheckDroid is able to ﬁnd common bad practices incurred in by newbie Android programmers.},
	language = {en},
	urldate = {2021-10-21},
	booktitle = {2017 {IEEE}/{ACM} 4th {International} {Conference} on {Mobile} {Software} {Engineering} and {Systems} ({MOBILESoft})},
	publisher = {IEEE},
	author = {Yovine, Sergio and Winniczuk, Gonzalo},
	month = may,
	year = {2017},
	pages = {175--176},
	file = {yovine2017.pdf:/home/q/Documents/papers/yovine2017.pdf:application/pdf},
}

@inproceedings{yusianto_route_2020,
	address = {Semarang, Indonesia},
	title = {The {Route} {Guidance} {System} using {Android}-{Based} {Navigation} to {Determine} the {Shortest} {Potatoes} {Distribution} {Route}},
	isbn = {978-1-72819-066-2 978-1-72819-068-6},
	url = {https://ieeexplore.ieee.org/document/9234244/},
	doi = {10.1109/iSemantic50169.2020.9234244},
	abstract = {At present, post-harvest loss is an increasingly interesting issue. When farmers have used quality seeds, improved on-farm handling and advanced harvesting technology, the researchers began to focus on post-harvest problems. In Indonesia, the potatoes post-harvest loss is high at 32.8 kg/ton. Transportation and distribution problems have an effect of 20.43\%. The contribution of this research is an Android-based advanced navigation system based on navigation radius for the route guidance system (RGS). We used the Dijkstra algorithm which we combined with latitude and longitude-based dynamic maps using Google APIs server for determining the shortest potatoes distribution route. To optimize the distance of the navigation radius, we used the radians approach on dynamic coordinates. The route that we calculated was all the nodes that were in the navigation radius. We can display distribution centers (DC) at a certain radius with the Google Maps fragment activity that embedded into the application and navigate to that place. So this method made it easy for decision makers to distribute their potatoes via the shortest route. The RGS using an android-based navigation that we proposed was implemented on a mobile application, and a comparison with the classical Dijkstra algorithm was performed. The results showed that this navigation system was better, more reliable with an accuracy rate of 99.83\%. This proved that the android-based navigation system that we developed can be used. For future research, spatial analysis needs to be considered.},
	language = {en},
	urldate = {2021-10-21},
	booktitle = {2020 {International} {Seminar} on {Application} for {Technology} of {Information} and {Communication} ({iSemantic})},
	publisher = {IEEE},
	author = {Yusianto, Rindra and {Marimin} and {Suprihatin} and Hardjomidjojo, Hartrisari},
	month = sep,
	year = {2020},
	pages = {320--326},
	file = {yusianto2020.pdf:/home/q/Documents/papers/yusianto2020.pdf:application/pdf},
}

@inproceedings{zhang_design_2019,
	address = {Chengdu, China},
	title = {Design and {Application} {Development} of the {Camps} {Navigation} {System} {Based} on {ArcGIS} {Runtime} {SDK} for {Android} : ——{Taking} the {Yunnan} {Normal} {University} as an example},
	isbn = {978-1-72811-907-6},
	shorttitle = {Design and {Application} {Development} of the {Camps} {Navigation} {System} {Based} on {ArcGIS} {Runtime} {SDK} for {Android}},
	url = {https://ieeexplore.ieee.org/document/8997730/},
	doi = {10.1109/IAEAC47372.2019.8997730},
	abstract = {With the development of Internet communication technology and the widespread popularity of smart-phone, the market for mobile navigation systems has been matured. It is categorized and analyzed by the mobile navigation system appearing in the current mobile navigation market, we found that there are inaccuracies and missing navigation data in the campus area. For this issue, the article uses ArcGIS Runtime and Android as the development platform. to develop campus navigation application software. The campus navigation map is produced by high-resolution remote sensing images, and it has high data space resolution, which effectively improves the data quality. This data uses ArcGIS Online for data cloud storage and management, achieved the interactive updates between data and users and solve the problem of complicated navigation data upgrade. Based on high-precision map data, the software design to achieve campus positioning, walking navigation, driving navigation, and point of interest searching. The mobile navigation system can realize high-precision navigation services in the campus area, so as to achieve better mapping resources for users, and facilitate the travel and life of visitors and college teachers and students on campus.},
	language = {en},
	urldate = {2021-10-21},
	booktitle = {2019 {IEEE} 4th {Advanced} {Information} {Technology}, {Electronic} and {Automation} {Control} {Conference} ({IAEAC})},
	publisher = {IEEE},
	author = {Zhang, Enwei and Peng, Shuangyun and Zhai, Yujie},
	month = dec,
	year = {2019},
	pages = {1262--1266},
	file = {zhang2019.pdf:/home/q/Documents/papers/zhang2019.pdf:application/pdf},
}

