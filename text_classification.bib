	note = {Conference Name: IEEE Transactions on Computational Social Systems},
	keywords = {Data models, Feature extraction, text classification, Bit error rate, fake news, Social networking (online), Bidirectional encoder representations from transformer (BERT), convolutional neural network (CNN), linguistic feature, Linguistics, machine learning (ML), Training, Vegetation, voting classifier, word embedding (WE)},
